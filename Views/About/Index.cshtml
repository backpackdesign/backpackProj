@using backpackProj.Models.classes
@model List<Hakkimizda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}
<section>
    <div class="content">
        <div class="wrap">
            <div class="about">
                <h3>Hakkımızda</h3>


                @foreach (var hakkimizda in Model)
                {<img src="@hakkimizda.FOTOURL" alt="" />
                    <p>@hakkimizda.ACIKLAMA</p>
                }
            </div>
        </div>
    </div>
</section>